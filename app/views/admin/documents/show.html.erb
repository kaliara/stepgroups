<p>
  <b>Title:</b>
  <%= @document.title %>
</p>

<% unless @document.intergroup_session.nil? %>
<p>
  <b>Intergroup session:</b>
  <%= link_to @document.intergroup_session.name, admin_intergroup_session_path(@document.intergroup_session) %>
</p>
<% end %>

<p>
  <b>Document type:</b>
  <%= @document.pretty_document_type %>
</p>


<%= link_to 'Edit', edit_admin_document_path(@document) %> |
<%= link_to 'Back', admin_documents_path %>
