<h2>Documents</h2>

<table>
  <tr>
    <th>Title</th>
    <th>Document type</th>
    <th>Intergroup session</th>
  </tr>

<% @documents.each do |document| %>
  <tr>
    <td><%= link_to document.title, document_path(document) %></td>
    <td><%= document.document_type %></td>
    <td><%= document.intergroup_session.name unless document.intergroup_session.nil? %></td>
  </tr>
<% end %>
</table>
