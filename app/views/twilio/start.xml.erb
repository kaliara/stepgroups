<?xml version="1.0" encoding="UTF-8"?>
<Response>
  <Gather method="GET" timeout="10" numDigits="1">
    <Say voice="woman">Thank you for calling N Y C Coda.</Say>
    <Pause />
    <Say voice="woman">For a list of meetings in New York City, press 1.</Say>
    <Pause />
    <Say voice="woman">For information about codependency, press 2.</Say>
    <Pause />
    <Say voice="woman">For information about coda, press 3.</Say>
    <Pause />
    <Say voice="woman">To leave us a message, press 4.</Say>
    <Pause />
    <Say voice="woman">To repeat these options, press 0.</Say>
  </Gather>
  <Say voice="woman">Please call back and try your selection again.</Say>
</Response>